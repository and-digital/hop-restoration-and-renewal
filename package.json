    "gatsby-transformer-sharp": "^2.3.16",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-gtm-module": "^2.0.8",
    "react-helmet-async": "^1.0.4",
    "react-obfuscate": "^3.6.6",
    "sharp": "^0.24.1"
  },
  "devDependencies": {
    "@storybook/react": "^5.3.14",
    "@testing-library/cypress": "^5.3.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^25.1.3",
    "axe-core": "^3.5.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-preset-gatsby": "^0.2.29",
    "cypress": "^4.1.0",
    "cypress-axe": "^0.5.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-kentcdodds": "^14.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-react": "^7.18.3",
    "gatsby-plugin-material-ui": "^2.1.6",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jest-chain": "^1.1.5",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.10.8"
  },
  "homepage": "https://github.com/contentful-userland/gatsby-contentful-starter#readme",
  "keywords": [
    "gatsby",
    "contentful"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contentful-userland/gatsby-contentful-starter.git"
  },
  "scripts": {
    "clean": "gatsby clean",
    "start": "gatsby develop",
    "build": "gatsby build --prefix-paths",
    "serve": "gatsby serve --prefix-paths",
    "serve-static": "npm run build && npm run serve",
    "test": "jest --watch",
    "storybook": "start-storybook",
    "test-coverage": "jest --coverage",
    "cy-open": "cypress open",
    "e2e": "CYPRESS_BASE_URL=http://localhost:8000 start-server-and-test start http://localhost:8000 cy-open",
    "cy-run": "cypress run --browser chrome --headless",
    "e2e-ci": "start-server-and-test serve http://localhost:9000/hop-restoration-and-renewal cy-run",
    "lint": "eslint --ext .js,.jsx --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore '**/*.+(js)'",
    "format": "npm run prettier -- --write",
    "create-component": "node ./scripts/createComponent.js",
    "create-template": "node ./scripts/createTemplate.js",
    "create-page": "node ./scripts/createPage.js",
    "create-hook": "node ./scripts/createHook.js"
  }
}
